package com.tamal.techgig.contest;

import java.util.Scanner;

public class VirusOutBreak {

	public static void main(String args[] ) throws Exception {

    	Scanner sc = new Scanner(System.in);
        String virusComposition = sc.nextLine();
        int numOfSample = Integer.parseInt(sc.nextLine());
        for(int i=1;i<numOfSample;i++) {
        	if(isSubSequence(sc.nextLine(),virusComposition, sc.nextLine().length(), virusComposition.length())) {
        		System.out.println("POSITIVE");
        	} else {
        		System.out.println("NEGATIVE");
        	}
        }
        

   }

	static boolean isSubSequence(String str1, String str2, int m, int n)
	{
		// Base Cases
		if (m == 0)
			return true;
		if (n == 0)
			return false;

		// If last characters of two strings are matching
		if (str1.charAt(m - 1) == str2.charAt(n - 1))
			return isSubSequence(str1, str2, m - 1, n - 1);

		// If last characters are not matching
		return isSubSequence(str1, str2, m, n - 1);
	}
}
